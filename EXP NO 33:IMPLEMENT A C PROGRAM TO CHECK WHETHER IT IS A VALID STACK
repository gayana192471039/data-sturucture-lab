#include <stdio.h>
#define SIZE 10
int stack[SIZE];
int top = -1;
int push(int x) {
    if (top == SIZE - 1) 
        return 0;
    stack[++top] = x;
    return 1;
}
int pop(int x) {
    if (top == -1) 
        return 0;
    if (stack[top] == x) {
        top--;
        return 1;
    }
    return 0;
}
int main() {
    int n, i, x;
    char op;
    printf("Enter number of operations: ");
    scanf("%d", &n);
    int valid = 1;
    for (i = 0; i < n; i++) {
        printf("Enter operation (P for push / O for pop): ");
        scanf(" %c %d", &op, &x);
 if (op == 'P') {
            if (!push(x))
                valid = 0;
        }
        else if (op == 'O') {
            if (!pop(x))
                valid = 0;
        }
    } if (valid)
        printf("It is a VALID STACK operation sequence.\n");
    else
        printf("It is NOT a valid stack sequence.\n");
return 0;
}
