#include <stdio.h>
unsigned long long factorial(unsigned int n) {
    unsigned long long res = 1;
    for (unsigned int i = 2; i <= n; ++i) res *= i;
    return res;
}
int main() {
    unsigned int n;
    printf("Enter non-negative integer: ");
    if (scanf("%u", &n) != 1) return 0;
    printf("%u! = %llu\n", n, factorial(n));
    return 0;
}
