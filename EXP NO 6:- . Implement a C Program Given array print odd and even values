#include <stdio.h>
int main() {
    int n;
    printf("Number of elements: ");
    if (scanf("%d", &n) != 1 || n <= 0) return 0;
    int a[n];
    for (int i = 0; i < n; ++i) scanf("%d", &a[i]);
    printf("Even numbers: ");
    for (int i = 0; i < n; ++i) if (a[i] % 2 == 0) printf("%d ", a[i]);
    printf("\nOdd numbers: ");
    for (int i = 0; i < n; ++i) if (a[i] % 2 != 0) printf("%d ", a[i]);
    printf("\n");
    return 0;
}
