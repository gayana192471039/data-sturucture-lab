#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node *next;
};

struct Node *hashTable[10];

// Insert function
void insert(int value, int size) {
    int key = value % size;
    struct Node *newNode = (struct Node*)malloc(sizeof(struct Node));
    newNode->data = value;
    newNode->next = NULL;

    if (hashTable[key] == NULL) {
        hashTable[key] = newNode;
    } else {
        struct Node *temp = hashTable[key];
        while (temp->next != NULL)
            temp = temp->next;
        temp->next = newNode;
    }
}

// Display hash table
void display(int size) {
    int i;
    for (i = 0; i < size; i++) {
        struct Node *temp = hashTable[i];
        printf("Index %d -> ", i);
        while (temp != NULL) {
            printf("%d -> ", temp->data);
            temp = temp->next;
        }
        printf("NULL\n");
    }
}

int main() {
    int size, n, value, i;

    printf("Enter hash table size: ");
    scanf("%d", &size);

    printf("Enter number of elements: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter value: ");
        scanf("%d", &value);
        insert(value, size);
    }

    display(size);
    return 0;
}
