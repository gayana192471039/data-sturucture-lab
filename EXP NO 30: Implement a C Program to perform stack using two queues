#include <stdio.h>
#include <stdlib.h>
#define SIZE 20
int q1[SIZE], q2[SIZE];
int front1 = -1, rear1 = -1;
int front2 = -1, rear2 = -1;

void enqueue1(int x) { q1[++rear1] = x; }
void enqueue2(int x) { q2[++rear2] = x; }
int dequeue1() { return q1[++front1]; }
int dequeue2() { return q2[++front2]; }
void push(int x) {
    enqueue2(x);
    while (front1 != rear1)
        enqueue2(dequeue1());
    while (front2 != rear2)
        enqueue1(dequeue2());
}
int pop() {
    if (front1 == rear1)
        return -1;
    return dequeue1();
}
int main() {
    push(10);
    push(20);
    push(30);
    printf("POP: %d\n", pop());
    printf("POP: %d\n", pop());
}
